{
  "id": "<%= pluginId %>",
  "name": "<%= pluginName %>",
  "version": "0.0.1",
  "description": "A basic plugin generated by the toolkit",
  "entryPoint": "./dist/index.js",
  "staticDir": "public",
<% if (includeFrontend) { -%>
  "client": {
    "remoteEntryRelativePath": "public/client/remoteEntry.js",
    "exposedModule": "./<%= pluginComponentName %>",
    "componentName": "<%= pluginComponentName %>",
    "hasDedicatedPage": true
  }
<% } -%>
} 